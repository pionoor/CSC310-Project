
Employees (

Employement_ID INT PRIMARY KEY,
Salary DECIMAL(4,2),
Name CHAR(25),
Position CHAR(10)
);

Financial Package (

PackageID INT PRIMARY KEY,
TotalAwardAmount INT
)

Colleges (
Name CHAR(25) PRIMARY KEY,
#FacultyMembers INT, 
#Students INT
)

Student(
UIN			INT PRIMARY KEY,
Major			VARCHAR(20),
Name			VARCHAR(25),
CompletedHRS	INT,
)

Professtional Society(
Societyname     VARCHAR(45) PRIMARY KEY,
Participants		INT,
ParentOrganization	VARCHAR(40)
)

Professional Society Member(
HomeAddress		VARCHAR(30) PRIMARY KEY,
Position		VARCHAR(10) PRIMARY KEY,
MemberName		VARCHAR(20) PRIMARY KEY,
Membersince		DATE
)
